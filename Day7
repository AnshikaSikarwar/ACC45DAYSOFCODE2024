#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int w,x,y,z;
	    cin>>w>>x>>y>>z;
	    if(w==x||w==y||w==z)
	    cout<<"Yes"<<endl;
	    else if(w==x+y)
	    cout<<"Yes"<<endl;
	    else if(w==x+z)
	    cout<<"Yes"<<endl;
	    else if(w==y+z)
	    cout<<"Yes"<<endl;
	    else if(w==x+y+z)
	    cout<<"Yes"<<endl;
	    else
	    cout<<"No"<<endl;
	}
}

