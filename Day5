#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T,p,q,r,s;
	std::cin >> T;
	while(T--){
	    cin>>p>>q>>r>>s;
	    int tp = p + q + r + s;

        int maxp= max({p,q,r,s});

        if (2*maxp > tp)
        cout<<"yes"<<endl;
        else
        cout<<"no"<<endl;
	}
}
